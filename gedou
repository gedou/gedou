int x = 10;
int y = 10;
int x1 = 0;
int y1 = 10;

void setup(){

size(400,400);

background(0);

}
void draw(){
rect(x,y,10,10);
 x += 10;

 if( x == 380 ){
   x -= 10;
   y += 10;
}
 if( y == 350 ){
   rect(x,y,10,10);
   x -= 20;
 }
 rect(x1,y1,10,10);
 y1 += 10;
 if( y1 == 350 ){
   y1 -= 10;
 }
}
